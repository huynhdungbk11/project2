<body>
  <div class="product-framgia">
    <div id="wrapper">
      <div id="page-wrapper" >
        <div id="page-inner">
          <div class="row">
            <div class="col-md-12">
              <h2><%= t "product_index_title" %></h2>
              <h5><%= t "product_index_title_small" %></h5>
            </div>
          </div>
          <hr>
          <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
              <div class="panel panel-default">
                <div class="panel-heading">
                  <%= t "product_table_title" %>
                </div>
                <div class="panel-heading">
                  <%= render "search" %>
                </div>
                <div class="panel-body">
                  <div class="table-responsive">
                    <%= render "list_order" %>
                </div>
              </div>
            </div>
            <%= paginate @orders %>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
